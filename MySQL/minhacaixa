 +create database minhacaixa;
 +use minhacaixa;
 +create table cliente (
 +  clienteCodigo int identity,
 +  ClienteNome varchar(50),
 +  rg int
 +  constraint PK_Cliente primary key (clienteCodigo, rg)
 +);
